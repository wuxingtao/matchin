{
  "name": "vue-geshop",
  "version": "1.0.0",
  "description": "restructure geshop with vue multiple page",
  "main": "index.js",
  "scripts": {
    "server": "webpack-dev-server --config  webpack/build/webpack.base.conf.js",
    "base": "webpack --config  webpack/build/webpack.base.conf.js",
    "dev": "webpack --config  webpack/build/webpack.dev.conf.js",
    "prod": "webpack --config  webpack/build/webpack.prod.conf.js",
    "debug": "node --inspect-brk node_modules/webpack/bin/webpack.js --config webpack/build/webpack.folder.conf.js",
    "debug-ios": "node --inspect-brk node_modules/.bin/webpack --config webpack/build/webpack.folder.conf.js",
    "folder": "webpack --config  webpack/build/webpack.folder.conf.js",
    "prebuild": "node webpack/build/build.js --platform=pc",
    "build": "npm run prebuild --inlineSource"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:xingtao/vue-geshop.git"
  },
  "author": "xingtao",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/runtime": "^7.3.1",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-glob-entry": "^2.1.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
